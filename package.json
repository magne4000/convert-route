{
  "name": "convert-route",
  "version": "0.1.2",
  "description": "Convert between rou3, Next.js, path-to-regexp and RegExp route patterns",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsdown -c tsdown.config.ts",
    "test": "vitest",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "release": "pnpm run build && changeset version && changeset tag && pnpm i && pnpm publish -r"
  },
  "type": "module",
  "keywords": [
    "rou3",
    "path-to-regexp",
    "next-fs",
    "regexp"
  ],
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/convert-route",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@changesets/cli": "^2.29.7",
    "next-route-matcher": "^1.0.2",
    "path-to-regexpv6": "npm:path-to-regexp@^6.3.0",
    "path-to-regexpv8": "npm:path-to-regexp@^8.3.0",
    "rou3": "^0.7.10" ,
    "tsdown": "^0.16.6",
    "typescript": "^5.9.3",
    "urlpattern-polyfill": "^10.1.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./rou3": "./dist/rou3.js",
    "./path-to-regexp-v6": "./dist/path-to-regexp-v6.js",
    "./path-to-regexp-v8": "./dist/path-to-regexp-v8.js",
    "./next-fs": "./dist/next-fs.js",
    "./regexp": "./dist/regexp.js"
  },
  "packageManager": "pnpm@10.11.0"
}
